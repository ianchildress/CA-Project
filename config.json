{
    "start": {
        "images": [
            "mysql",
            "ca-web",
            "ca-web",
            "ca-web"
        ],
        "containers": [],
        "settings": {
            "stop-containers": true
        }
    },
    "configs": {
        "containers": [
            {
                "id": "web1",
                "hostconfig": {
                    "Links": [
                        "civis-mysql"
                    ]
                }
            }
        ],
        "images": [
            {
                "image": "ca-web",
                "options": {
                    "name": "",
                    "config": {
                        "Image": "ca-web"
                    },
                    "hostconfig": {
                        "Links": [
                            "civis-mysql"
                        ]
                    }
                }
            },
            {
                "image": "mysql",
                "options": {
                    "name": "civis-mysql",
                    "config": {
                        "Image": "mysql",
                        "Env":["MYSQL_ROOT_PASSWORD=civis"]
                    }
                }
            }
        ]
    }
}